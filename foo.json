{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "array-type": [
      2,
      "array"
    ],
    "arrow-return-shorthand": [
      2,
      "multiline"
    ],
    "arrow-parens": 0,
    "class-name": 2,
    "comment-format": [
      1,
      "check-space",
      "check-lowercase"
    ],
    "curly": 2,
    "eofline": 0,
    "indent": [
      2,
      2
    ],
    "interface-over-type-literal": 0,
    "interface-name": 0,
    "jsdoc-format": 2,
    "match-default-export-name": 2,
    "max-file-line-count": [2, 200],
    "max-line-length": [2, 200],
    "max-classes-per-file": [2, 3],
    "member-access": 2,
    "member-ordering": 0,
    "new-parens": 2,
    "newline-before-return": 0,
    "no-consecutive-blank-lines": 0,
    "no-console": 0,
    "no-debugger": 2,
    "no-duplicate-imports": 2,
    "no-duplicate-super": 2,
    "no-duplicate-switch-case": 2,
    "no-eval": 2,
    "no-invalid-this": 2,
    "no-namespace": 0,
    "no-non-null-assertion": 2,
    "no-parameter-properties": 2,
    "no-shadowed-variable": 2,
    "no-string-throw": 2,
    "no-switch-case-fall-through": 2,
    "no-this-assignment": [
      2,
      {
        "allow-destructuring": 2
      }
    ],
    "no-trailing-whitespace": 2,
    "no-var-requires": 0,
    "number-literal-format": 2,
    "object-literal-sort-keys": 0,
    "one-line": 2,
    "ordered-imports": 0,
    "prefer-function-over-method": 0,
    "prefer-object-spread": 2,
    "prefer-switch": 2,
    "prefer-template": 2,
    "quotemark": [
      2,
      "single",
      "jsx-single",
      "avoid-escape"
    ],
    "radix": 2,
    "semicolon": 2,
    "trailing-comma": [
      2,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "triple-equals": 2,
    "use-isnan": 2,
    "variable-name": [
      2,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "whitespace": [
      2,
      "check-decl",
      "check-operator",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-preblock"
    ]
  }
}